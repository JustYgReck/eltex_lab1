objects = main.o f1.o f2.o
libs = liblab1.a
rezultStatic : $(objects) $(libs)
	gcc $(object) -o rezultStatic -L. -llab1
main.o : main.c
	gcc -c main.c
liblab1.a : f1.o f2. o
	ar rc liblab1.a f1.o f2.o
f1.o : f1.c
	gcc -c f1.c
f2.o : f2.c
	gcc -c f2.c
#
.PHONY : clean
clean : 
	-rm rezultStatic $(libs) $(objects)